{
    "system": {
        "ilvoProcesses": [
            {
                "Name": "ilvo-navigation",
                "Running": true,
                "SoftwareUpdate": true,
                "CheckHeartbeat": true
            },
            {
                "Name": "ilvo-operation",
                "Running": true,
                "SoftwareUpdate": true,
                "CheckHeartbeat": true
            },
            {
                "Name": "ilvo-simulation",
                "Running": true,
                "SoftwareUpdate": true,
                "CheckHeartbeat": true
            },
            {
                "Name": "ilvo-robot-plc",
                "Running": true,
                "SoftwareUpdate": true,
                "CheckHeartbeat": false
            },
            {
                "Name": "ilvo-gps",
                "Running": true,
                "SoftwareUpdate": true,
                "CheckHeartbeat": false
            }
        ],
        "ilvoAddons": [
            {
                "Name": "system",
                "Running": true,
                "SoftwareUpdate": true,
                "DockerRegistry": {
                    "serveraddress": "https://registry.hub.docker.com/v2/"
                },
                "DockerConfig": {
                    "Image": "axelwillekens/artof-system",
                    "HostConfig": {
                        "Binds": [
                            "/d/artof-docker/docker/data:/var/lib/ilvo"
                        ],
                        "RestartPolicy": {
                            "Name": "on-failure",
                            "MaximumRetryCount": 3
                        },
                        "PortBindings": {
                            "80/tcp": [
                                {
                                    "HostPort": "8080"
                                }
                            ]
                        },
                        "NetworkMode": "ilvo_artof_network"
                    },
                    "ExposedPorts": {
                        "80/tcp": {}
                    },
                    "Env": [
                        "REDIS_HOST=redis"
                    ]
                }
            },
            {
                "Name": "node-red",
                "Running": true,
                "SoftwareUpdate": true,
                "DockerRegistry": {
                    "serveraddress": "https://registry.hub.docker.com/v2/"
                },
                "DockerConfig": {
                    "Image": "nodered/node-red",
                    "HostConfig": {
                        "Binds": [
                            "/d/artof-docker/docker/data/node-red:/data",
                            "/d/artof-docker/docker/data/field:/var/lib/ilvo/field",
                            "/d/artof-docker/docker/data/implement:/var/lib/ilvo/implement",
                            "/d/artof-docker/docker/data/config.json:/var/lib/ilvo/config.json",
                            "/d/artof-docker/docker/data/settings.json:/var/lib/ilvo/settings.json",
                            "/d/artof-docker/docker/data/types.json:/var/lib/ilvo/types.json",
                            "/d/artof-docker/docker/data/redis.init.json:/var/lib/ilvo/redis.init.json"
                        ],
                        "RestartPolicy": {
                            "Name": "on-failure",
                            "MaximumRetryCount": 3
                        },
                        "PortBindings": {
                            "1880/tcp": [
                                {
                                    "HostPort": "1880"
                                }
                            ]
                        },
                        "NetworkMode": "ilvo_artof_network"
                    },
                    "ExposedPorts": {
                        "1880/tcp": {}
                    }
                }
            }
        ]
    },
    "variables": {
        "pc.simulation.fix": 4,
        "pc.simulation.factor": 1.0,
        "pc.simulation.active": true,
        "pc.navigation.non_operational_velocity": 1.0,
        "pc.navigation.operational_velocity": 1.0,
        "pc.navigation.sideways_velocity": 0.2,
        "pc.navigation.spinning_velocity": 0.2,
        "pc.navigation.max_angular_velocity": 10.0,
        "pc.navigation.mode": 1,
        "pc.navigation.spin_angle": 25.0,
        "pc.navigation.turning_radius": 3.0,
        "pc.purepursuit.carrot_distance": 3.5,
        "pc.purepursuit.inter_point_distance": 0.1,
        "pc.purepursuit.weight_factor": 1.0,
        "pc.purepursuit.pid.saturation.min": -1.0,
        "pc.purepursuit.pid.saturation.max": 1.0,
        "pc.field.name": "example"
    }
}
